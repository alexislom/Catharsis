<?xml version="1.0" encoding="UTF-8"?>
<rxui:ReactiveContentPage
             xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
             xmlns:viewModels="clr-namespace:CatharsisSampleForms.ViewModels;assembly=CatharsisSampleForms"
             x:TypeArguments="viewModels:RedPageViewModel"
             x:Class="CatharsisSampleForms.Views.RedPage">
    <ContentPage.Content>
        <StackLayout BackgroundColor="Red">
            <Button x:Name="PopModal" Text="Pop Modal"/>
            <Button x:Name="PushPage" Text="Push Another Red Page"/>
            <Button x:Name="PopPage" Text="Pop Page"/>
            <Button x:Name="PopToRoot" Text="Pop To Root"/>
        </StackLayout>
    </ContentPage.Content>
</rxui:ReactiveContentPage>