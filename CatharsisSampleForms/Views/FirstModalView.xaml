<?xml version="1.0" encoding="UTF-8"?>
<rxui:ReactiveContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:rxui="clr-namespace:ReactiveUI.XamForms;assembly=ReactiveUI.XamForms"
             xmlns:viewModels="clr-namespace:CatharsisSampleForms.ViewModels;assembly=CatharsisSampleForms"
             x:TypeArguments="viewModels:FirstModalViewModel"
             x:Class="CatharsisSampleForms.Views.FirstModalView"
             Title="First Modal">
    <ContentPage.Content>
        <StackLayout>
            <Button x:Name="OpenSecondModal" Text="Second Modal" />
            <Button x:Name="PopModal" Text="Pop Modal" />
        </StackLayout>
    </ContentPage.Content>
</rxui:ReactiveContentPage>